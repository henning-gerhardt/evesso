<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDefault>
            <ROOT>
                <TCEforms>
                    <sheetTitle>EVE SSO Login Plugin Configuration</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.loginSystem>
                        <TCEforms>
                            <label>EVE SSO Login System</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">TQ</numIndex>
                                        <numIndex index="1">https://login.eveonline.com</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">SISI</numIndex>
                                        <numIndex index="1">https://sisilogin.testeveonline.com</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.loginSystem>
                    <settings.clientId>
                        <TCEforms>
                            <label>Client ID</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.clientId>
                    <settings.secretKey>
                        <TCEforms>
                            <label>Secret Key</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.secretKey>
                    <settings.showCallbackLink>
                        <TCEforms>
                            <label>Show callback link</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.showCallbackLink>
                </el>
            </ROOT>
        </sDefault>
    </sheets>
</T3DataStructure>